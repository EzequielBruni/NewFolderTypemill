{# The (very basic) markup for individual files/pages. #}

{% include 'partials/breadcrumb.twig' %}



    <section id="pagecontent">

        {% if settings.themes.NewFolderTypemill.pagelayout == "layoutsideleft" %}

            <header class="layoutsideleftheader" >
                <div class="headerwrap">
                    <h1>{{ title }}</h1>
                </div>
            </header>

        {% endif %}

        <div class="pagewrap">

            <div class="content">
                <article>

                    {% if settings.themes.NewFolderTypemill.pagelayout == "layoutcentered" %}

                        <header>
                            <h1>{{ title }}</h1>
                        </header>

                    {% endif %}
                    
                    {{ content }}
                    
                </article>

            </div>

        </div>
    </section>